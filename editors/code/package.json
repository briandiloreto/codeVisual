{
  "name": "codevisual",
  "displayName": "Visual",
  "description": "Code Visualization",
  "version": "0.4.0",
  "publisher": "Atomic Concepts Software",
  "author": {
    "name": "Brian DiLoreto"
  },
  "icon": "contentCrab/icon.png",
  "repository": {
    "url": "https://github.com/bdiloreto/codevisual"
  },
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages",
    "Visualization"
  ],
  "keywords": [
    "call graph"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codevisual.generateCallGraph",
        "title": "Visual: Generate Call Graph"
      },
      {
        "command": "codevisual.generateFuncCallGraph",
        "title": "Visual: Generate Function Call Graph"
      },
      {
        "command": "codevisual.exportCallGraph",
        "title": "Export Call Graph"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "codevisual.generateCallGraph",
          "when": "never"
        },
        {
          "command": "codevisual.generateFuncCallGraph",
          "when": "never"
        },
        {
          "command": "codevisual.exportCallGraph",
          "when": "never"
        }
      ],
      "editor/context": [
        {
          "command": "codevisual.generateFuncCallGraph",
          "group": "codevisual",
          "when": "editorHasCallHierarchyProvider"
        }
      ],
      "explorer/context": [
        {
          "command": "codevisual.generateCallGraph",
          "group": "codevisual"
        }
      ],
      "webview/context": [
        {
          "command": "codevisual.exportCallGraph",
          "when": "webviewId == 'codevisual.callgraph'"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "test-extension": "node ./out/test/extension.test.js",
    "test-vscode": "vscode-test"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.17.7",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "~1.83.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@wasm-tool/wasm-pack-plugin": "^1.6.0",
    "eslint": "^8.57.0",
    "glob": "^10.3.12",
    "mocha": "^10.4.0",
    "raw-loader": "^4.0.2",
    "ts-loader": "^9.4.2",
    "typescript": "^5.4.5",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.1.3"
  },
  "dependencies": {
    "@viz-js/viz": "^3.4.0",
    "@vscode/codicons": "^0.0.32",
    "@vscode/webview-ui-toolkit": "^1.0.0",
    "d3": "^7.3.0",
    "d3-graphviz": "^4.0.0",
    "ignore": "^5.3.0",
    "jquery": "^3.6.0",
    "jquery-color": "^2.2.0",
    "jquery-mousewheel": "^3.1.13",
    "lodash": "^4.17.21",
    "typescript-collections": "^1.3.3",
    "vscode-uri": "^3.0.8"
  }
}
